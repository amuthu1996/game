<%= simple_form_for(@register) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id %>
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :auth_token %>
    <%= f.input :auth_token_expire %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :source %>
    <%= f.input :college %>
    <%= f.input :course %>
    <%= f.input :year %>
    <%= f.input :phone %>
    <%= f.input :custom %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
