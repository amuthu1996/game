<p id="notice"><%= notice %></p>

<h1>Listing Registers</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Email</th>
      <th>Password</th>
      <th>Auth token</th>
      <th>Auth token expire</th>
      <th>City</th>
      <th>State</th>
      <th>Source</th>
      <th>College</th>
      <th>Course</th>
      <th>Year</th>
      <th>Phone</th>
      <th>Custom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registers.each do |register| %>
      <tr>
        <td><%= register.id %></td>
        <td><%= register.name %></td>
        <td><%= register.email %></td>
        <td><%= register.password %></td>
        <td><%= register.auth_token %></td>
        <td><%= register.auth_token_expire %></td>
        <td><%= register.city %></td>
        <td><%= register.state %></td>
        <td><%= register.source %></td>
        <td><%= register.college %></td>
        <td><%= register.course %></td>
        <td><%= register.year %></td>
        <td><%= register.phone %></td>
        <td><%= register.custom %></td>
        <td><%= link_to 'Show', register %></td>
        <td><%= link_to 'Edit', edit_register_path(register) %></td>
        <td><%= link_to 'Destroy', register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Register', new_register_path %>
